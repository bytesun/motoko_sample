    "https://rosetta-api.internetcomputer.org/search/transactions",
    {
      body: JSON.stringify({
        network_identifier: {
          blockchain: "Internet Computer",
          network: "00000000000000020101",
        },
        account_identifier: {
          address: "abc-dss-...",
        },
      }),
      method: "POST",
      headers: {
        "content-type": "application/json;charset=UTF-8",
      },
    },
  )
